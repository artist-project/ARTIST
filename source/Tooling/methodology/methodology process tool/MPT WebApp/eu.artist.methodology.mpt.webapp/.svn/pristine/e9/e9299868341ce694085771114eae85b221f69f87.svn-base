	<script>
	function changeframe(loc) {
		   
		   var project = document.getElementById('mainForm:selectedProject').textContent;
		   
		   //alert(project);
		   
		   if (loc == "Choose_project.xhtml" ||  (loc == "AddTeamMember.xhtml")  || (project != null &amp;&amp; project != "")) {
		     document.getElementById('myframe').src = loc;
		   } else {
		    alert("Please choose a project first.");
		   }
