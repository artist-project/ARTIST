	 */
	quantitative MeanTimeBetweenFailures {
		description "",
		type MARTE_Library.BasicNFP_Types.NFP_Duration,
		direction decreasing
	}
	
	/**
	 * 
	 */
	quantitative MeanTimeToFailure {
		description "",
		type MARTE_Library.BasicNFP_Types.NFP_Duration,
		direction decreasing
	}
	
	/**
