	quantitative DownTimeInPercent {
		description "",
		type MARTE_Library.BasicNFP_Types.NFP_Percentage,
		direction decreasing
	}
	
	/**
	 * 
	 */
	quantitative MeanTimeToRestoreService {
		description "",
		type MARTE_Library.BasicNFP_Types.NFP_Duration,
		direction decreasing
	}
	
	/**
