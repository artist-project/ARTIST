		direction decreasing
	}
	
	/**
	 * 
	 */
	quantitative ResponseTime {
		description "ResponseTime",
		type MARTE_Library.BasicNFP_Types.NFP_Duration,
		direction decreasing
	}
	
	/**
	 * 
	 */
	quantitative Throughput {
		description "",
